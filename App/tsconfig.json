{
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNEXT",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "API/dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
